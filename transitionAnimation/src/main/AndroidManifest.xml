<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liliuhuan.teststringformate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".anime.OverridePendingActivity" />
        <activity
            android:name=".AnimeActivity"
            android:theme="@style/styleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".anime.StyleActivity"
            android:theme="@style/styleActivity" />
        <activity android:name=".anime.ActivityOptionsActivity" />
        <activity android:name=".anime.SlidActivity"></activity>
    </application>

</manifest>